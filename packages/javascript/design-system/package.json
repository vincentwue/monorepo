{
  "name": "@monorepo/design-system",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": "./src/styles.css",
    "./styles.css": "./src/styles.css",
    "./tailwind-preset": {
      "types": "./src/tailwind-preset.d.ts",
      "default": "./src/tailwind-preset.js"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "default": "./dist/tokens.js"
    },
    "./native": {
      "types": "./dist/native/index.d.ts",
      "default": "./dist/native/index.js"
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
